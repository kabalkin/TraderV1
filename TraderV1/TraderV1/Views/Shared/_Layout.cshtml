@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TraderV1</title>

  
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" /> 
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/js/charts/style.css" />

    <script src="~/js/charts/amcharts.js"></script>
    <script src="~/js/charts/serial.js"></script>
    <script src="~/js/charts/amstock.js"></script>
    <script src="~/js/charts/plugins/dataloader/dataloader.min.js"></script>
    <script src="~/js/charts/plugins/export/export.min.js"></script>
    <link rel="stylesheet" href="~/js/charts/plugins/export/export.css" type="text/css" media="all" />
    <script src="~/js/charts/themes/dark.js"></script>
  
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/signalr.min.js"></script>
    <script src="~/js/notify.min.js"></script>

    

</head>
<body>

<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">TraderV1</a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a>Вы вошли как: @User.Identity.Name</a></li>
                <li><a asp-area="" asp-controller="Account" asp-action="Logout">Выйти</a></li>

            </ul>
        </div>
    </div>
</nav>



<div class="container body-content">

    @RenderBody()

</div>






</body>
</html>

<script>
  
</script>